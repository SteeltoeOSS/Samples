{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Information",
      "Steeltoe": "Trace"
    }
  },
  "spring": {
    "application": {
      "name": "musicstore"
    },
    "cloud": {
      "config": {
        "uri": "http://localhost:8888",
        "validate_certificates": false
      }
    }
  },
  "hystrix": {
    "command": {
      "default": {
        "circuitBreaker": {
          "requestVolumeThreshold": 3
        }
      }
    },
    "stream": {
      "validate_certificates": false
    }
  },
  "management": {
    "endpoints": {
      "cloudfoundry": {
        "validateCertificates": false
      }
    }
  },
  "sqlserver": {
    "credentials": {
      "database": "MusicStore"
    }
  },
  "DefaultAdminUsername": "Administrator@test.com",
  "DefaultAdminPassword": "YouShouldChangeThisPassword1!"
}
