{
  "$schema": "https://steeltoe.io/schema/v3/schema.json",
  "Spring": {
    "Application": {
      "Name": "foo"
    },
    "Cloud": {
      "Config": {
        "HealthEnabled": true,
        "DiscoveryEnabled": true,
        "Uri": "http://localhost:8888",
        "ValidateCertificates": false
      }
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Management": {
    "Endpoints": {
      "Actuator": {
        "Exposure": {
          "Include": [ "*" ],
          "Exclude": [ "env", "refresh" ]
        }
      }
    }
  }
  // Discovery-first
  //,"Eureka": {
  //  "Client": {
  //    "ServiceUrl": "http://localhost:8761/eureka/"
  //  }
  //}
}
