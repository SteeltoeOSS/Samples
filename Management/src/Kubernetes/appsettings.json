{
  "$schema": "https://steeltoe.io/schema/v3/schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Spring": {
    "Boot": {
      "Admin": {
        "Client": {
          "Url": "http://host.docker.internal:9090",
          "BasePath": "http://host.docker.internal:5000"
        }
      }
    }
  },
  "Management": {
    "Endpoints": {
      "Actuator": {
        "Exposure": {
          "Include": [ "*" ],
          "Exclude": [ "env", "refresh" ]
        }
      }
    }
  }
}
