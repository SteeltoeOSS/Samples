{
  "$schema": "https://steeltoe.io/schema/v3/schema.json",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Steeltoe": "Information"
    },
    "Console": {
      "DisableColors": true
    }
  },
  "Management": {
    "Endpoints": {
      "Path": "/cloudfoundryapplication",
      "CloudFoundry": {
        "ValidateCertificates": false
      }
    },
    "metrics": {
        "exporter": {
            "cloudfoundry": {
                "validateCertificates": false
              }
          }
      }
    },
    "info": {
        "appsetting1": "appsetting1",
        "appsetting2": "appsetting2",
        "appsetting3": "appsetting3",
        "appsetting4": "appsetting4"
    },
    "MySql": {
      "Client": {
        "SslMode": "none"
      }
    },
    "HealthChecksUI": {
        "HealthChecks": [
            {
                "Name": "HTTP-Api-Basic",
                "Uri": "http://localhost:58672/health"
            }
        ],
        "Webhooks": [
            {
                "Name": "",
                "Uri": "",
                "Payload": "",
                "RestoredPayload": ""
            }
        ],
        "EvaluationTimeOnSeconds": 10,
        "MinimumSecondsBetweenFailureNotifications": 60
    }
  }
