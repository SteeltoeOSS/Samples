{
  // Steeltoe: Add schema to get auto-completion.
  "$schema": "https://steeltoe.io/schema/v3/schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  // Steeltoe: Enable Kubernetes service bindings.
  "Steeltoe": {
    "Kubernetes": {
      "Bindings": {
        "Enable": true
      }
    }
  },
  "AllowedHosts": "*"
}
