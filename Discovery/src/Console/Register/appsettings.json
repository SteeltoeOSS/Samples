{
  "$schema": "https://steeltoe.io/schema/v3/schema.json",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Steeltoe": "Debug"
    }
  },
  "Eureka": {
    "Client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "ShouldRegisterWithEureka": true,
      "ShouldFetchRegistry": false
    },
    "Instance": {
      "port": 5000,
      "AppName": "MyApp",
      "InstanceId": "Instance-5000",
      "IsInstanceEnabledOnInit": true
    }
  }
}