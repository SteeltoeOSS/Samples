{
  "$schema": "https://steeltoe.io/schema/v3/schema.json",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Steeltoe": "Debug"
    }
  },
  "Security": {
    "Oauth2": {
      "Client": {
        "additionalScopes": "testgroup",
        "saveTokens": true,
        "ValidateCertificates": false,
        "TokenValidationParameters": {
          "ValidateAudience":  true
        }
      }
    }
  }
}

